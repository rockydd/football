<h1>Listing players</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for user in @users%>
  <tr>
    <td><%= image_tag user.avatar.url(:tiny) %></td>
    <td><%= link_to user.login,user_path(user) %></td>
    <td><%= link_to 'Show', user%></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New team', new_team_path %>
